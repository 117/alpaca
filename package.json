{
  "name": "@master-chief/alpaca",
  "version": "3.7.0",
  "description": "A TypeScript Node.js library for the https://alpaca.markets REST API and WebSocket streams.",
  "author": "117",
  "contributors": [
    "lbstr",
    "AqilCont",
    "KalebMills",
    "anonrose",
    "husayt"
  ],
  "license": "ISC",
  "keywords": [
    "alpaca",
    "alpaca-trade-api-js",
    "alpaca trade api",
    "trading",
    "exchange",
    "bot",
    "alpaca markets",
    "markets",
    "stocks",
    "stock"
  ],
  "type": "module",
  "types": "./@types/index.d.ts",
  "main": "./dist/cjs/index.cjs",
  "exports": {
    "import": "./dist/mjs/index.mjs",
    "require": "./dist/cjs/index.cjs"
  },
  "scripts": {
    "build": "npm run clean && npm i && npm run build:mjs && npm run build:cjs",
    "build:mjs": "tsc --outDir dist/mjs --moduleResolution node --allowSyntheticDefaultImports -d --declarationDir @types --module esnext --target esnext src/*.ts && map dist/mjs js mjs",
    "build:cjs": "tsc --outDir dist/cjs --moduleResolution node --esModuleInterop src/*.ts && map dist/cjs js cjs",
    "clean": "rimraf dist @types",
    "test": "npm run test:default",
    "test:default": "jest",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ],
    "testMatch": [
      "**/*.test.ts"
    ]
  },
  "dependencies": {
    "@types/ws": "^7.4.0",
    "bottleneck": "^2.19.5",
    "events": "^3.2.0",
    "isomorphic-ws": "^4.0.1",
    "ky": "^0.26.0",
    "ky-universal": "^0.9.1",
    "qs": "^6.9.4",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "@master-chief/map": "0.0.4",
    "@types/jest": "^26.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/qs": "^6.9.3",
    "jest": "^26.4.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  }
}
